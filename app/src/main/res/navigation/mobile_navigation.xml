<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/dashboard"
    tools:ignore="UnusedNavigation">


    <fragment
        android:id="@+id/dashboard"
        android:name="com.example.androshare.Dashboard"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/nearMe"
        android:name="com.example.androshare.NearMe"
        android:label="fragment_near_me"
        tools:layout="@layout/fragment_near_me" />
    <fragment
        android:id="@+id/favourites"
        android:name="com.example.androshare.Favourites"
        android:label="fragment_favourites"
        tools:layout="@layout/fragment_favourites" />
    <fragment
        android:id="@+id/appSettings"
        android:name="com.example.androshare.AppSettings"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/profile"
        android:name="com.example.androshare.Profile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/logout"
        android:name="com.example.androshare.Logout"
        android:label="fragment_logout"
        tools:layout="@layout/fragment_logout" />
    <fragment
        android:id="@+id/more"
        android:name="com.example.androshare.More"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" />

</navigation>